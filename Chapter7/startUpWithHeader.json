[
  {
    "description": "这是一个带header头信息的post请求",
    "request": {
      "uri": "/post/with/headers",
      "method": "post",
      "headers": {
        "Content-type": "application/json"
      },
      "json": {
        "name": "zhangsan",
        "age": "18"
      }
    },
    "response": {
      "status": 201,
      "json": {
        "name": "zhangsan",
        "age": "18"
      }
    }
  }
]