<?xml version="1.0" encoding="UTF-8" ?>
<suite name="groupsOnClass">

    <test name="runAll">
        <classes>
            <class name="cn.ztt.code.groups.GroupsOnClass1"/>
            <class name="cn.ztt.code.groups.GroupsOnClass2"/>
            <class name="cn.ztt.code.groups.GroupsOnClass3"/>
        </classes>

    </test>

    <test name="onlyRunStu">
        <groups>
            <run>
                <include name = "stu"/>
            </run>
        </groups>
        <classes>
            <class name="cn.ztt.code.groups.GroupsOnClass1"/>
            <class name="cn.ztt.code.groups.GroupsOnClass2"/>
            <class name="cn.ztt.code.groups.GroupsOnClass3"/>
        </classes>


    </test>

</suite>
